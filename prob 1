#include <iostream>
#include <conio.h>

using namespace std;

int main ()
{
    int arr[10], n, i, max, min;
    
    cout << "Enter Array Size : " << endl;
    cin >> n;
    cout << "Enter Array Elements : " << endl;
      for (i = 0; i < n; i++)
    cin >> arr[i];
    
    
    max = arr[0];
      for (i = 0; i < n; i++)
    {
        if (max < arr[i])
            max = arr[i];
    }
    min = arr[0];
      for (i = 0; i < n; i++)
    {
        if (min > arr[i])
            min = arr[i];
    }
   _getch();
    return 0;
}
